<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root type="AnchorPane" prefWidth="341.0" xmlns="http://javafx.com/javafx/8.0.45" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="TOP_CENTER" layoutY="45.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Text fx:id="title" strokeType="OUTSIDE" strokeWidth="0.0" textOrigin="TOP">
            <font>
                <Font size="20.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="15.0" top="15.0"/>
            </VBox.margin>
        </Text>
        <ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" prefHeight="1000.0" prefWidth="200.0">
            <VBox fx:id="pane" alignment="CENTER" minWidth="-Infinity" prefWidth="320.0" spacing="15">
                <padding>
                    <Insets top="15"/>
                </padding>
            </VBox>
        </ScrollPane>
    </VBox>
</fx:root>